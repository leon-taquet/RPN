.section init 
         .temps r6,r7
         .zero r0
         r0 := 0

         start:
                r1 := 12
                r2 := 10
                output r1 + 48
                output " mod "
                output r2 + 48
                output " = "
                r3 := r1 mod r2
                output r3 + 48
                output "\n"
                halt